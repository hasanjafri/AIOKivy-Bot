#:kivy 1.10.1

<ProductListDropDown>:
    Button:
        text: 'My first Item'
        size_hint_y: None
        height: 44
        on_release: root.select('item1')
    Label:
        text: 'Unselectable item'
        size_hint_y: None
        height: 44
    Button:
        text: 'My second Item'
        size_hint_y: None
        height: 44
        on_release: root.select('item2')

<AIOTabbed>:
    size_hint: .5, .5
    pos_hint: {'center_x': .5, 'center_y': .5}
    do_default_tab: False

    TabbedPanelItem:
        text: 'Nike'
        Label:
            text: 'First tab content area'
    TabbedPanelItem:
        text: 'Adidas'
        BoxLayout:
            Label:
                text: 'Second tab content area'
            Button:
                text: 'Button that does nothing'
    TabbedPanelItem:
        text: 'Supreme'
        GridLayout:
            rows: 11
            cols: 2
            padding: 10
            spacing: 10
            Label:
                text: 'Product Type:'
            ProductListDropDown:
            